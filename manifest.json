{
  "manifest_version": 2,

  "name": "Trello List Time Totals",
  "version": "0.1.0",
  "description": "See how much time you need on a given day",

  "content_scripts": [{
    "css": ["styles.css"],
    "js": ["jquery-3.3.1.min.js", "content.js"],
    "matches": ["https://trello/b/*"],
    "run_at": "document_end"
  }]

}
